#: C/zenity.xml:1166(programlisting)
#, no-wrap
msgid ""
"\n"
"        #!/bin/sh\n"
"\n"
"        FILE=`zenity --file-selection \\\n"
"          --title=\"Select a File\"`\n"
"\n"
"        case $? in\n"
"                 0)\n"
"                        zenity --text-info \\\n"
"                          --title=$FILE \\\n"
"                          --filename=$FILE \\\n"
"                          --editable 2&gt;/tmp/tmp.txt;;\n"
"                 1)\n"
"                        echo \"No file selected.\";;\n"
"                -1)\n"
"                        echo \"No file selected.\";;\n"
"        esac\n"
"      "
